db.feed.find({group_id: '__all__', posted_by:'7b732aa2fdc011e3acbb22000a7c88834', company_id: '44388244b2af11e5802c0800275eaa7c1', '_id': ObjectId("57072cbe6617646a29ffb9de")}).explain("allPlansExecution")

